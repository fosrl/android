<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- When switch is checked (on) - use Pangolin primary color (orange) -->
    <item android:color="#FFF15232" android:state_checked="true"/>
    <!-- When switch is unchecked (off) - use light gray that shows on black -->
    <item android:color="#FFBDBDBD"/>
</selector>